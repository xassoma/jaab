programa
{
	
	funcao inicio()
	{
		real nota1, nota2, nota3, calculo 
		
		escreva ("Digite a primeira nota: ")
		leia (nota1)

		escreva ("Digite a segunda nota: ")
		leia (nota2) 

		escreva ("Digite a terceira nota: ")
		leia (nota3) 
		calculo = ((nota1 * 2) + (nota2 * 3) + (nota3 * 5)) / 3 

		escreva ("A média das 3 notas é: ", calculo)
	}
}
